{
  "name": "@rsc-cache/next",
  "version": "0.1.0-beta",
  "private": true,
  "exports": {
    ".": {
      "react-server": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": ["dist/**/*.{js,d.ts}"],
  "type": "module",
  "types": "./dist/index.d.ts",
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "build": "pnpm build:swc && pnpm types",
    "build:swc": "swc src -d dist --copy-files",
    "dev": "pnpm build:swc -w",
    "types": "tsc --emitDeclarationOnly"
  },
  "devDependencies": {
    "@rsc-cache/eslint-config": "workspace:*",
    "@rsc-cache/typescript-config": "workspace:*",
    "@types/eslint": "^8.44.7",
    "@types/node": "^20.5.2",
    "@types/react": "^18.2.5",
    "@types/react-dom": "^18.2.3",
    "eslint": "^8.53.0",
    "react": "18.3.0-canary-c5b937576-20231219",
    "react-dom": "18.3.0-canary-c5b937576-20231219",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "next": "^14.0.4",
    "react": "canary",
    "react-dom": "canary"
  },
  "dependencies": {
    "server-only": "^0.0.1"
  },
  "bugs": {
    "url": "https://github.com/fredkiss3/rsc-cache/issues"
  }
}
