{
  "name": "@rsc-cache/next",
  "version": "0.2.0",
  "exports": {
    ".": {
      "react-server": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*.{js,d.ts}"
  ],
  "type": "module",
  "types": "./dist/index.d.ts",
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "build": "pnpm build:swc && pnpm types",
    "build:swc": "swc src -d dist --copy-files",
    "dev": "pnpm build:swc -w",
    "types": "tsc --emitDeclarationOnly"
  },
  "devDependencies": {
    "@rsc-cache/eslint-config": "workspace:*",
    "@rsc-cache/typescript-config": "workspace:*",
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.101",
    "@types/eslint": "^8.44.7",
    "@types/node": "^20.5.2",
    "@types/react": "^18.2.5",
    "@types/react-dom": "^18.2.3",
    "eslint": "^8.53.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "next": "^14.0.4",
    "react": ">= 18.2.0",
    "react-dom": ">= 18.2.0"
  },
  "dependencies": {
    "server-only": "^0.0.1"
  },
  "bugs": {
    "url": "https://github.com/fredkiss3/rsc-cache/issues"
  },
  "homepage": "https://github.com/fredkiss3/rsc-cache#readme",
  "license": "MIT"
}
