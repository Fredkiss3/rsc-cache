{
  "compilerOptions": {
    "outDir": "dist",
    "target": "ES6",
    "module": "ES6",
    "jsx": "preserve",
    "types": ["@types/react/canary", "@types/react-dom/canary", "node"],
    "lib": ["dom", "dom.iterable", "esnext"],
    "noUncheckedIndexedAccess": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "allowJs": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "incremental": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "emitDeclarationOnly": true,
    "isolatedModules": true
  },
  "include": [ "src", "src/global.d.ts", "src/rsdw.d.tsd.ts"],
  "exclude": ["node_modules", "dist"]
}
